<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css'/>
  </head>
  <body>
    <h1>Welcome to express </h1>

    <% include templates/header.ejs %>

    <p><%= name %></p>

    <p><%= points %></p>

    <h3>This is json data</h3>

    <h3><%= videodata.categoryName %></h3>
    <ul>
      <% videodata.categories.forEach(function(item) { %>
        <li><a href="video?id=<%=item.categoryID%>"><%= item.categoryName %></li>

      <% }); %>

    </ul>

  </body>
</html>
